<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}

    </head>
    <body>
    {% for type, msgs in app.flashes %}
        {% for message in msgs %}
            Message: {{type}} - {{ message }}
            <br>
        {% endfor %}
    {% endfor %}
    <br>
<di class="container">
    <div class="row">

        <div class="col-sm-2" >
            <div id="app">vue block</div>

            <ul class="d-block float-left">
                <li>menu</li>
                <li><a href="{{path('app_main')}}">index</a></li>
                <li><a href="{{path('app_login')}}">login</a></li>
                <li><a href="{{path('app_register')}}">register</a></li>
                <li><a href="{{path('app_web_basket')}}">basket</a></li>
                <li><a href="{{path('app_web_product')}}">shop</a></li>
            </ul>
        
            <ul class="d-block float-left">
                <li>admin</li>
                <li><a href="{{path('app_product_index')}}">products</a></li>
                <li><a href="{{path('app_tax_index')}}">tax</a></li>
                <li><a href="{{path('app_address_index')}}">address</a></li>
                <li><a href="{{path('app_payment_index')}}">paymants</a></li>
            </ul>
        </div>

        <div class="col-sm-10">
            <div class="container">
                {% block body %}{% endblock %}

            </div>
        </div>

    </div>
</di>

    </body>
</html>
